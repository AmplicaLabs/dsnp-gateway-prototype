{
  "name": "dsnp-gateway-prototype",
  "version": "0.0.0",
  "description": "A DSNP Gateway API service that links to Frequency",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "npm run build && node dist/index.js",
    "watch": "npx nodemon --exec ts-node --esm index.ts --project ./tsconfig.json",
    "test": "jest",
    "format": "tsc --noEmit --pretty && eslint --fix \"**/*.ts\"",
    "lint": "tsc --noEmit --pretty && eslint \"**/*.ts\"",
    "gen:types": "npx openapi-client-axios-typegen ./openapi.json > types/openapi.d.ts",
    "local:init": "node scripts/local-init.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmplicaLabs/dsnp-gateway-prototype.git"
  },
  "author": "Amplica Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AmplicaLabs/dsnp-gateway-prototype/issues"
  },
  "homepage": "https://github.com/AmplicaLabs/dsnp-gateway-prototype#readme",
  "dependencies": {
    "@digitalbazaar/ed25519-signature-2020": "^5.2.0",
    "@digitalbazaar/ed25519-verification-key-2020": "^4.1.0",
    "@digitalbazaar/vc": "^6.0.2",
    "@dsnp/activity-content": "../activity-content/dist",
    "@dsnp/frequency-schemas": "../schemas/dist",
    "@dsnp/parquetjs": "^1.3.1",
    "@frequency-chain/api-augment": "^1.7.2",
    "@multiformats/blake2": "^1.0.13",
    "@polkadot/api": "^10.9.1",
    "@polkadot/util": "^12.3.2",
    "@polkadot/util-crypto": "^12.3.2",
    "ajv": "^8.12.0",
    "avro-js": "^1.11.2",
    "busboy": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "jsonld-signatures": "^11.2.1",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multiformats": "^12.0.1",
    "openapi-backend": "^5.9.2",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/mime-types": "^2.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/wait-on": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "axios": "^1.4.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "openapi-client-axios-typegen": "^7.2.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "wait-on": "^7.0.1"
  }
}
