{
  "name": "dsnp-gateway-prototype",
  "version": "0.0.0",
  "description": "A DSNP Gateway API service that links to Frequency",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "npm run build && node dist/index.js",
    "watch": "npx nodemon --exec ts-node --esm index.ts --project ./tsconfig.json",
    "test": "jest",
    "format": "tsc --noEmit --pretty && eslint --fix \"**/*.ts\"",
    "lint": "tsc --noEmit --pretty && eslint \"**/*.ts\"",
    "gen:types": "npx openapi-client-axios-typegen ./openapi.json > types/openapi.d.ts",
    "local:init": "node scripts/local-init.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmplicaLabs/dsnp-gateway-prototype.git"
  },
  "author": "Amplica Labs",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AmplicaLabs/dsnp-gateway-prototype/issues"
  },
  "homepage": "https://github.com/AmplicaLabs/dsnp-gateway-prototype#readme",
  "dependencies": {
    "@dsnp/activity-content": "^1.1.0",
    "@dsnp/frequency-schemas": "^1.1.0",
    "@dsnp/parquetjs": "^1.6.0",
    "@frequency-chain/api-augment": "^1.11.0",
    "@multiformats/blake2": "^2.0.2",
    "@polkadot/api": "^10.12.1",
    "@polkadot/util": "^12.6.2",
    "@polkadot/util-crypto": "^12.6.2",
    "avro-js": "^1.11.3",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "multiformats": "^13.1.0",
    "openapi-backend": "^5.10.6",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/busboy": "^1.5.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "axios": "^1.6.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "openapi-client-axios-typegen": "^7.6.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "wait-on": "^7.2.0"
  }
}
